<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="correspondingLabel" name="Corresponding Label Configuration" schema="schema/correspondingLabel.exsd"/>
   <extension-point id="labelFinderStrategy" name="Corresponding Label Strategy" schema="schema/labelFinderStrategy.exsd"/>
   <extension-point id="bindingSupportProvider" name="BindingSupportProvider" schema="schema/bindingSupportProvider.exsd"/>

    <extension
         point="org.eclipse.riena.core.annotatedMethodHandlers">
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnActionCallback"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.ActionCallbackAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnClick"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.ClickAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnSelectionChange"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.SelectionChangedAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnFocusGained"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.FocusGainedAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnFocusLost"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.FocusLostAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnPropertyChange"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.PropertyChangeAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnDoubleClick"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.DoubleClickAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnActionCallbacks"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.ActionCallbacksAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnClicks"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.ClicksAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnDoubleClicks"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.DoubleClicksAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnFocusGaineds"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.FocusGainedsAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnFocusLosts"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.FocusLostsAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnPropertyChanges"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.PropertyChangesAnnotationHandler">
      </annotatedMethodHandler>
      <annotatedMethodHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.OnSelectionChanges"
            class="org.eclipse.riena.internal.ui.ridgets.annotation.handler.SeletionChangedsAnnotationHandler">
      </annotatedMethodHandler>

   </extension>
    <extension
          point="org.eclipse.riena.core.logCatchers">
       <logCatcher
             class="org.eclipse.riena.internal.ui.ridgets.databinding.DatabindingLogCatcher"
             name="Databinding LogCatcher">
       </logCatcher>
    </extension>
   
</plugin>
